{

    "name": "Codechef Checker",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "webRequest","tabs",
        "*://*.google.com/"
      ],
      "background": {
        "scripts": ["background.js"],
        "persistent": true
      },
      "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["contentScript.js"]
        }
      ]


}