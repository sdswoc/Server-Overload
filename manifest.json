{
  "name": "Codechef Checker",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
      "default_popup": "popup.html",
      
      "default_title": "perfWatch",
      "default_icon": { "128": "logo.png"}
  },
  
  "icons": {
      "128": "logo.png"
  },
  "permissions": ["notifications","storage",
      "webRequest","tabs","activeTab","declarativeNetRequest","webRequestBlocking","declarativeNetRequestFeedback",
      "https://www.codechef.com/api/ide/*","https://www.codechef.com/error_status_table/*"
      
    ],
    "background": {
      "service_worker": "background.js"
      
    },
    "content_scripts": [
      {
        "matches": [ "https://www.codechef.com/submit/*"],
        "js": ["contentScript.js"]
      }
    ]


}